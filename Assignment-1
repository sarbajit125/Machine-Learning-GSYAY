F.
nl=[]
for x in range(150, 270):
    if (x%7==0) and (x%5==0):
        nl.append(str(x))
print (','.join(nl))
G.
def Remove(duplicate):
    final_list = []
    for num in duplicate:
        if num not in final_list:
            final_list.append(num)
    return final_list
     
# Driver Code
duplicate = [10,20,30,40,50,60,70]
print("original list is \n",duplicate)
print("New list is \n",Remove(duplicate))
